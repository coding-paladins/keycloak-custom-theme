{
  "name": "keycloak-custom-theme",
  "version": "0.0.0",
  "description": "keycloak custom theme",
  "type": "module",
  "scripts": {
    "postinstall": "keycloakify sync-extensions",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-keycloak-theme": "npm run build && keycloakify build",
    "start-keycloak": "keycloakify start-keycloak",
    "keycloak:compose": "pnpm run build-keycloak-theme && docker compose up",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:account": "eslint src/account",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run format"
  },
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
